<h1>Login to your account below.</h1>
<%= form_tag(sessions_path) do %>

<div class='field'>
    <%= label_tag :kerberos %><%= text_field_tag :kerberos, params[:kerberos] %>
    <%= label_tag :password %><%= password_field_tag :password, params[:password] %>
</div>

<div class='Submit'><%= submit_tag 'Login' %></div>

<% end %>
